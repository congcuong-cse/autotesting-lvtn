<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
 <extension
         point="org.eclipse.ui.editors">
      <editor
            class="de.htwg.flowchartgenerator.editor.FlowChartEditor"
            default="true"
            extensions="atg"
            icon="icons/plugin-icon16.png"
            id="PluginATG.editor1"
            name="ATG Chart">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.jdt.core.IMethod"
            id="PluginATG.contribution1">
         <menu
               label="Auto Test Generator"
               path="additions"
               id="PluginATG.menu1">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="de.htwg.flowchartgenerator.popup.actions.CoverAction"
               enablesFor="1"
               id="PluginATG.newAction"
               label="From last CodeCover session"
               menubarPath="PluginATG.menu1/group1">
         </action>
         <action
               class="de.htwg.flowchartgenerator.popup.actions.CoverFromAction"
               enablesFor="1"
               icon="icons/calendar.png"
               id="PluginATG.newAction"
               label="From CodeCover session"
               menubarPath="PluginATG.menu1/group1">
         </action>
         <action
               class="de.htwg.flowchartgenerator.popup.actions.MainAction"
               enablesFor="1"
               icon="icons/plugin-icon16.png"
               id="PluginATG.newAction"
               label="Build"
               menubarPath="PluginATG.menu1/group1">
         </action>
      </objectContribution>
   </extension>

</plugin>
